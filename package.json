{
  "name": "pgen",
  "version": "1.0.0",
  "main": "./dist/pgen.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/schiehll/pgen.git"
  },
  "author": "Reinaldo Schiehll <rn.schiehll@gmail.com> (http://github.com/schiehll)",
  "scripts": {
    "clean": "rimraf dist",
    "clean:test": "rimraf dist/test",
    "build:prod": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:dev": "npm run clean && cross-env NODE_ENV=development webpack --config webpack.config.dev.js",
    "build:test": "npm run clean:test && cross-env NODE_ENV=test webpack --config webpack.config.test.js",
    "build": "npm run build:dev",
    "start": "node devServer.js",
    "test": "npm run build:test && mocha ./dist/test/test.js",
    "test:all": "cross-env TESTING=all npm run build:test && mocha ./dist/test/test.js"
  },
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-plugin-typecheck": "^3.6.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-register": "^6.4.3",
    "cross-env": "^1.0.6",
    "enzyme": "^2.0.0-rc1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "glob": "^6.0.4",
    "imports-loader": "^0.6.5",
    "jsdom": "^7.2.2",
    "json-loader": "^0.5.4",
    "jsx-control-statements": "^3.0.0",
    "react-addons-test-utils": "^0.14.6",
    "rimraf": "^2.4.3",
    "should": "^8.1.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "webpack-umd-external": "^1.0.2"
  },
  "dependencies": {
    "freezer-js": "^0.9.6",
    "random-js": "^1.0.8",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "snapsvg": "^0.4.0"
  }
}
